{
  "name": "passe-un-dessin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@formatjs/intl-pluralrules": "4.3.2",
    "@reduxjs/toolkit": "1.7.2",
    "@sentry/react": "6.18.0",
    "@sentry/tracing": "6.18.0",
    "awesome-debounce-promise": "2.1.0",
    "babel-polyfill": "6.26.0",
    "copy-to-clipboard": "3.3.1",
    "core-js": "3.21.1",
    "date-fns": "2.28.0",
    "env-cmd": "10.1.0",
    "formik": "2.2.9",
    "lz-string": "1.4.4",
    "postcss-scss": "4.0.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "5.24.6",
    "react-is": "17.0.2",
    "react-redux": "7.2.6",
    "react-router": "6.2.1",
    "react-router-dom": "6.2.1",
    "react-scripts": "5.0.0",
    "react-use": "17.3.2",
    "redux": "4.1.2",
    "regenerator-runtime": "0.13.9",
    "styled-components": "5.3.3",
    "use-constant": "1.1.0"
  },
  "scripts": {
    "audit-dependencies": "check-audit --yarn",
    "start": "REACT_APP_ENV=local react-app-rewired start --host 0.0.0.0",
    "build": "env-cmd -f .env.${REACT_APP_ENV} react-app-rewired build",
    "test": "react-app-rewired test --maxWorkers=4",
    "e2e:open": "cypress open",
    "e2e": "cypress run",
    "eject": "react-app-rewired eject",
    "analyze": "env-cmd -f .env.local react-app-rewired build --analyze-bundle",
    "test:coverage": "npm run test -- --coverage",
    "generate": "plop --plopfile scripts/generators/index.js",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --max-warnings 0",
    "lint:fix": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "stylelint": "stylelint 'src/**/*.style.{ts,tsx}' 'src/**/style.{ts,tsx}'",
    "type-check": "tsc"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/**/actions.js",
      "!src/**/index.js",
      "!src/**/serviceWorker.js",
      "!src/**/*.wrap.js",
      "!src/index.js",
      "!src/tempPolyfills.js",
      "!src/setupTests.js",
      "!src/redux/reducers.js",
      "!src/redux/sagas.js",
      "!src/redux/store.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    }
  },
  "bundlesize": [
    {
      "path": "./build/static/js/*.js",
      "maxSize": "200 kB"
    }
  ],
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/lz-string": "1.3.34",
    "@types/node": "16.11.25",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/react-intl": "3.0.0",
    "@types/react-redux": "7.1.22",
    "@types/styled-components": "5.1.23",
    "@types/superagent": "4.1.15",
    "@types/webpack-env": "1.16.3",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "@welldone-software/why-did-you-render": "6.2.3",
    "babel-plugin-styled-components": "2.0.5",
    "bundlesize": "0.18.1",
    "customize-cra": "1.0.0",
    "cypress": "9.5.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-chai-friendly": "0.7.2",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-mysticatea": "4.2.4",
    "eslint-plugin-prefer-object-spread": "1.2.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-risxss": "2.1.0",
    "express": "4.17.3",
    "http-proxy-middleware": "2.0.3",
    "npm-audit-resolver": "2.3.1",
    "prettier": "2.5.1",
    "react-app-rewire-webpack-bundle-analyzer": "1.1.0",
    "react-app-rewired": "2.2.1",
    "sitemap-webpack-plugin": "1.1.1",
    "stylelint": "14.5.3",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "25.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-declaration-strict-value": "1.8.0",
    "postcss-scss": "4.0.3",
    "postcss-syntax": "0.36.2",
    "stylelint-config-standard-scss": "3.0.0",
    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "4.5.5",
    "webpack-manifest-plugin": "4.1.1"
  }
}
