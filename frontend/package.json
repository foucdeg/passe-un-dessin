{
  "name": "passe-un-dessin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@formatjs/intl-pluralrules": "3.4.10",
    "@reduxjs/toolkit": "1.4.0",
    "babel-polyfill": "6.26.0",
    "copy-to-clipboard": "3.3.1",
    "core-js": "3.6.5",
    "date-fns": "2.16.1",
    "env-cmd": "10.1.0",
    "formik": "2.2.1",
    "lz-string": "1.4.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-intl": "5.8.6",
    "react-is": "17.0.1",
    "react-redux": "7.2.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.0",
    "react-use": "15.3.4",
    "redux": "4.0.5",
    "regenerator-runtime": "0.13.7",
    "styled-components": "5.2.0",
    "superagent": "6.1.0"
  },
  "scripts": {
    "audit-dependencies": "check-audit --yarn",
    "start": "REACT_APP_ENV=local react-app-rewired start --host 0.0.0.0",
    "build": "env-cmd -f .env.${REACT_APP_ENV} react-app-rewired build",
    "test": "react-app-rewired test --maxWorkers=4",
    "eject": "react-app-rewired eject",
    "analyze": "env-cmd -f .env.${REACT_APP_ENV} react-app-rewired build --analyze-bundle",
    "test:coverage": "npm run test -- --coverage",
    "generate": "plop --plopfile scripts/generators/index.js",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "lint-style": "stylelint 'src/**/*.style.{ts,tsx}' 'src/**/style.{ts,tsx}'",
    "type-check": "tsc"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/**/actions.js",
      "!src/**/index.js",
      "!src/**/serviceWorker.js",
      "!src/**/*.wrap.js",
      "!src/index.js",
      "!src/tempPolyfills.js",
      "!src/setupTests.js",
      "!src/redux/reducers.js",
      "!src/redux/sagas.js",
      "!src/redux/store.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    }
  },
  "bundlesize": [
    {
      "path": "./build/static/js/*.js",
      "maxSize": "200 kB"
    }
  ],
  "devDependencies": {
    "@types/jest": "26.0.15",
    "@types/lz-string": "1.3.34",
    "@types/node": "12.19.1",
    "@types/react": "16.9.53",
    "@types/react-dom": "16.9.8",
    "@types/react-intl": "3.0.0",
    "@types/react-redux": "7.1.9",
    "@types/react-router": "5.1.8",
    "@types/react-router-dom": "5.1.6",
    "@types/styled-components": "5.1.4",
    "@types/superagent": "4.1.10",
    "@types/webpack-env": "1.15.3",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "@welldone-software/why-did-you-render": "5.0.0",
    "babel-plugin-styled-components": "1.11.1",
    "bundlesize": "0.18.0",
    "cypress": "5.4.0",
    "eslint-config-prettier": "6.14.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-mysticatea": "4.2.4",
    "eslint-plugin-prefer-object-spread": "1.2.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-risxss": "1.1.1",
    "http-proxy-middleware": "1.0.6",
    "npm-audit-resolver": "2.2.1",
    "prettier": "2.1.2",
    "react-app-rewire-webpack-bundle-analyzer": "1.1.0",
    "react-app-rewired": "2.1.6",
    "sitemap-webpack-plugin": "0.8.1",
    "stylelint": "13.7.2",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-declaration-strict-value": "1.6.0",
    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "4.0.3",
    "webpack-manifest-plugin": "2.2.0"
  }
}
